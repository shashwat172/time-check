{
    "manifest_version": 2,
  
    "name": "Time-check",
    "description": "This extension helps you keep track of time spent on each website",
    "version": "1.0",
    "icons": {
      "128": "clock128.png",
      "48": "clock48.png",
      "16": "clock16.png"
    },
  
    "browser_action": {
      "default_icon": "clock16.png",
      "default_popup": "popup.html"
    },

    "background":{
        "scripts":["eventPage.js"],
        "persistent":true
    },

    "permissions": [
        "tabs"
            ]

  }